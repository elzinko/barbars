                                    <td class="code_col subtotal_col">&nbsp;</td>
                                {{/if}}

                                {{#if ../../../../../additional_info}}
                                    <td class="text_right additional_info_col subtotal_col">&nbsp;</td>
                                {{/if}}

                                <td class="text_right quantity_col subtotal_col">
                                    {{#eq kind 'subtotal_with_quantity'}}
                                      {{quantity}}
                                    {{/eq}}
                                </td>

                                {{#if ../../../../../discount}}
                                    <td class="text_right discount_col subtotal_col">&nbsp;</td>
                                {{/if}}

                                {{#if ../../../../../tax_visible}}
                                    {{#unless ../../../../../../hide_tax}}
                                        <td class="text_right no_wrap number price_net_col subtotal_col">&nbsp;</td>
                                        {{#if ../../../../../../../show_unit_price_gross}}
                                            <td class="text_right no_wrap number price_gross_col subtotal_col">&nbsp;</td>
                                        {{/if}}
                                        <td class="text_right no_wrap number total_price_net_col subtotal_col">
                                            {{total_price_net}}
                                        </td>

                                        <td class="text_right no_wrap number tax_col subtotal_col">&nbsp;</td>
                                        {{#if ../../../../../../../tax2_visible}}
                                            <td class="text_right no_wrap number tax2_col subtotal_col">&nbsp;</td>
                                        {{/if}}
                                        {{#if ../../../../../../../tax2_details}}
                                            <td class="text_right number no_wrap tax_value_col tax1_value_col subtotal_col">&nbsp;</td>
                                            <td class="text_right number no_wrap tax_value_col tax2_value_col subtotal_col">&nbsp;</td>
                                        {{else}}
                                            <td class="text_right number no_wrap tax_value_col subtotal_col">
                                                {{tax_value}}
                                            </td>
                                        {{/if}}
                                    {{else}}
