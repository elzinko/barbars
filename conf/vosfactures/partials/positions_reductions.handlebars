					<td class="width3 text_right number total_price_gross_col footer total">
					{{#not_eq kind 'final'}}
						{{total_price_gross}}
					{{/not_eq}}
					{{#eq kind 'final'}}
						{{positions_total_price_gross}}
					{{/eq}}
					</td>
				</tr>

				{{#if show_tax_split}}

					{{#if tax_visible}}
						{{#unless hide_tax}}
							{{#each summary}}
								<tr class="no_border">
									<td class="nr_col footer subtotal"></td>
									<td class="name_col footer subtotal"></td>
									{{#if ../../use_product_code}}
										<td class="code_col footer subtotal"></td>
									{{/if}}
									{{#if ../../additional_info}}
										<td class="additional_info_col footer subtotal"></td>
									{{/if}}
									{{#if ../../discount}}
										<td class="discount_col footer subtotal"></td>
									{{/if}}
									<td class="quantity_col footer subtotal"></td>
									<td class="empty price_net_col footer subtotal" style="display: none;"></td>
                                    {{#if ../../show_unit_price_gross}}
                                        <td class="empty mod2 footer subtotal"></td>
                                    {{/if}}
									<td class="text_right number including footer subtotal">
										<strong>{{../../tax_name}}</strong>
									</td>

									<td class="text_right number total_price_net_col footer subtotal">
										{{total_price_net}}
									</td>

									<td class="text_right number tax_col footer subtotal">
										{{tax}}{{#not_eq tax 'N/A'}}%{{/not_eq}}
									</td>
									{{#if ../../../tax2_visible}}
										<td class="text_right number tax2_col footer subtotal">
											{{tax2}}{{#not_eq tax2 'N/A'}}%{{/not_eq}}
										</td>
									{{/if}}
									<td class="text_right number tax_value_col footer subtotal">
										{{tax_value}}
									</td>
									<td class="width3 text_right number total_price_gross_col footer subtotal">
										{{total_price_gross}}
									</td>
								</tr>
							{{/each}}
						{{/unless}}
					{{/if}}

				{{/if}}
			{{/not_eq}}
		</tbody>
	</table>

	{{#eq kind 'advance'}}
		<table class="clean to_right" style="width:auto;" id="positions_summary" align="right">
			{{#if show_tax_split}}

			{{#if tax_visible}}
			{{#unless hide_tax}}
			{{#each summary}}
			<tr class="no_border">
				<td class="text_right number including">
					<strong>{{../../tax_name}}</strong>
				</td>
				<td class="text_right number tax_col-advance footer">
					{{tax}}%
				</td>
				<td class="text_right number tax_value_col-advance footer">
					{{tax_value}}
				</td>
				<td class="text_right number total_price_gross_col-advance footer">
					{{total_price_gross}}
				</td>
			</tr>
			{{/each}}
			{{/unless}}
			{{/if}}

			{{/if}}
			<tr class="clean no_border">
