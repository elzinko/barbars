								</th> --}}
							{{else}}
								<td class="text_right no_wrap number price_gross_col-hide_tax price_gross_col">
										{{#if ../../../../../discount}}
											{{unit_price_gross_with_discount}}<br />
											<small style="text-decoration:line-through;">{{unit_price_gross}}</small>
										{{else}}
											{{unit_price_gross}}
										{{/if}}
									</td>
								<td class="text_right number total_price_gross_col">
									{{total_price_gross}}
								</td>
							{{/if}}
						{{else}}
							{{#unless ../../../../../hide_tax}}
								<td class="text_right number no_wrap total_price_net_col-advance">
									{{total_price_net}}
								</td>
							{{else}}
								<td class="text_right number no_wrap total_price_gross_col-advance">
									{{total_price_gross}}
								</td>
							{{/unless}}
						{{/not_eq}}
					</tr>
				{{/in}}
                {{/eq}}
				{{/not_eq}}

			{{/each}}


			{{#not_eq kind 'advance'}}
				<tr class="clean no_border summary_row">
					<td class="empty nr_col footer total"></td>
					<td class="empty name_col footer total"></td>
					{{#if use_product_code}}
						<td class="empty code_col footer total"></td>
					{{/if}}
					{{#if additional_info}}
						<td class="empty additional_info_col footer total"></td>
					{{/if}}
					{{#if discount}}
						<td class="empty discount_col footer total"></td>
					{{/if}}
					<td class="empty quantity_col quantity_footer footer total"></td>
                    {{#if show_unit_price_gross}}
                        {{#if tax_visible }}
                            {{#unless hide_tax }}
                                <td class="empty mod2 footer subtotal"></td>
                            {{/unless}}
                        {{/if}}
                    {{/if}}

