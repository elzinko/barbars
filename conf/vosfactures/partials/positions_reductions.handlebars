	<table class="main_inv_table full_width" id="pos_table">

		<thead>
			<tr>
				<th class="width1 no_wrap nr_col nr_col" id="nr_col">
					{{tt "invoice_position.nr"}}
				</th>
				<th class="width4 text_center name_col" id="name_col">
					{{tt "invoice_position.name"}}
				</th>

				{{#not_eq kind 'advance'}}
					{{#if use_product_code}}
						<th class="width2 text_center code_col" id="code_col">
							{{tt "invoice_position.code"}}
						</th>
					{{/if}}

					{{#if additional_info}}
						<th class="width2 text_center additional_info_col" id="additional_info_col">
							{{additional_info_desc}}
						</th>
					{{/if}}

					<th class="width2 text_center no_wrap quantity_col" id="quantity_col">
						{{tt "invoice_position.quantity"}}
					</th>

					{{#if discount}}
						<th class="width2 text_center no_wrap discount_col" id="discount_col">
							{{tt "invoice_position.discount_percent"}}
						</th>
					{{/if}}


					{{#if tax_visible}}
						{{#unless hide_tax}}
							<th class="width3 text_center no_wrap price_net_col" id="price_net_col">
								{{tt "invoice_position.price_net"}}
							</th>
                            {{#if show_unit_price_gross}}
                                <th class="width3 text_center no_wrap price_gross_col">
                                    {{tt "invoice_position.price_gross"}}
                                </th>
                            {{/if}}
							<th class="width3 text_center total_price_net_col" id="total_price_net_col">
								{{tt "invoice_position.total_price_net"}}
							</th>
							<th class="width1 text_center no_wrap tax_col" id="tax_col">
								{{tax_name}}
							</th>
							{{#if tax2_visible}}
								<th class="width1 text_center tax2_col" id="tax2_col">
									{{tax2_name}}
								</th>
							{{/if}}
							<th class="width3 text_center tax_value_col" id="tax_value_col">
								{{tax_value_name}}
							</th>
							{{#if tax2_details}}
								<th class="width3 text_center tax2_value_col" id="tax_value_col">
									{{tax2_value_name}}
								</th>
							{{/if}}
						{{else}}
							<th class="width3 no_wrap text_center price_gross_col price_gross_col-hide_tax" id="price_gross_col">
								{{tt "invoice_position.price_gross"}}
							</th>
						{{/unless}}
						<th class="width3 no_wrap text_center total_price_gross_col" id="total_price_gross_col">
							{{tt "invoice_position.total_price_gross"}}
						</th>
					{{else}}
						<th class="width3 no_wrap text_center price_net_col" id="price_net_col">
							{{tt "invoice_position.bill_price_net"}}
						</th>
						{{#unless hide_tax}}
							<th class="width3 no_wrap text_center total_price_net_col" id="total_price_gross_col">
								 {{tt "invoice_position.total_price_net"}}
							</th>
						{{else}}
							<th class="width3 no_wrap text_center total_price_gross_col" id="total_price_gross_col">
							 {{tt "invoice_position.total_price_gross"}}
						</th>
						{{/unless}}
					{{/if}}
				{{else}}
					{{#unless hide_tax}}
						<th class="width3 no_wrap text_center total_price_net_col-advance" id="total_price_net_col">
							{{tt "invoice_position.total_price_net"}}
						</th>
					{{else}}
						<th class="width3 no_wrap text_center total_price_gross_col-advance" id="total_price_gross_col">
							{{tt "invoice_position.total_price_gross"}}
						</th>
					{{/unless}}
				{{/not_eq}}
			</tr>
		</thead>
		<tbody>
			{{#each positions}}
			{{#eq kind 'page_break'}}
		</tbody>
	</table>
	<br><div style="page-break-after:always !important;"></div>
